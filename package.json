    "ci": "npm run clean; npm install; npm test; npm run analyse"
  },
  "devDependencies": {
    "grunt": "~0.4.1",
    "grunt-cli": "~0.1.9",
    "grunt-browserify": "~1.2.1",
    "grunt-istanbul": "~0.2.1",
    "buster": "~0.6.12",
    "buster-istanbul": "~0.1.10",
    "plato": "~0.6.1"
  },
